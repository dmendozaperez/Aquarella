<table id="result" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Orden</th>
            <th>Padre</th>
            <th align="center">Editar</th>
            <th align="center">Aplicación</th>
        </tr>
    </thead>
    <tbody data-bind="foreach:">
        @using www.aqmvc.com.pe.Data.Control
        @model IEnumerable<Funcion>

        @foreach (Funcion item in Model)
        {
            <tr>
                <td>@item.fun_id.ToString()</td>
                <td>@item.fun_nombre.ToString()</td>
                <td>@item.fun_orden</td>
                <td>@item.fun_padre</td>
                <td align="center"><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="anchorDetail btn btn-primary btn-xs" data-title="Edit" data-id="" data-toggle="modal" data-target="#edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>
                <td align="center"><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="anchorDetail btn btn-primary btn-xs" data-title="Edit" data-id="" data-toggle="modal" data-target="#edit"><span class="glyphicon glyphicon-list-alt"></span></button></p></td>
                @*<td><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="anchorDetail btn btn-primary btn-xs" data-title="Edit" data-id="@item.usu_id" data-toggle="modal" data-target="#edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>*@
            </tr>

        }
    </tbody>
    @if (Model.Count() != 0)
    {
        <tfoot>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Orden</th>
                <th>Padre</th>
                <th>Editar</th>
                <th>Aplicacion</th>
            </tr>
        </tfoot>
    }
</table>

